<%- model_class = ErrorReport -%>
<div class="page-header">
  <h1><%=t '.title', :default => ErrorReport %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:staff_actions) %></dt>
      <dd><pre class="prettyprint"><%= @error_report.staff_actions %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:time_out) %></dt>
      <dd><pre class="prettyprint"><%= @error_report.time_out %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:time_back) %></dt>
      <dd><pre class="prettyprint"><%= @error_report.time_back %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:equipment_type) %></dt>
      <dd><pre class="prettyprint"><%= @error_report.equipment_type %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:out_of_service) %></dt>
      <dd><pre class="prettyprint"><%= @error_report.out_of_service %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:needs_maintenance) %></dt>
      <dd><pre class="prettyprint"><%= @error_report.needs_maintenance %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:is_closed) %></dt>
      <dd><pre class="prettyprint"><%= @error_report.is_closed %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:cctv_off) %></dt>
      <dd><pre class="prettyprint"><%= @error_report.cctv_off %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:cctv_on) %></dt>
      <dd><pre class="prettyprint"><%= @error_report.cctv_on %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:affected_equipment) %></dt>
      <dd><pre class="prettyprint"><%= @error_report.affected_equipment %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:type_of_problem) %></dt>
      <dd><pre class="prettyprint"><%= @error_report.type_of_problem %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:owning_agency) %></dt>
      <dd><pre class="prettyprint"><%= @error_report.owning_agency %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:outside_support) %></dt>
      <dd><pre class="prettyprint"><%= @error_report.outside_support %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:reporting_agency) %></dt>
      <dd><pre class="prettyprint"><%= @error_report.reporting_agency %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:ticket_number) %></dt>
      <dd><pre class="prettyprint"><%= @error_report.ticket_number %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:operator_actions) %></dt>
      <dd><pre class="prettyprint"><%= @error_report.operator_actions %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to  "Index", error_reports_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_error_report_path(@error_report), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              error_report_path(@error_report),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

